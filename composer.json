{
    "name": "radarlog/micker",
    "description": "A microservice calculating the Mars Sol Date (MSD) and the Martian Coordinated Time (MTC) based on the time on Earth in UTC",
    "keywords": [
        "REST",
        "SOLID",
        "Mars Sol Date",
        "Martian Coordinated Time"
    ],
    "type": "project",
    "license": "MIT",
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": "~7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Radarlog\\Micker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Radarlog\\Micker\\Tests\\": "tests/"
        }
    }
}
